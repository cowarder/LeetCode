class Solution {
public:
    bool hasCycle(ListNode *head){
        ListNode* a=head;
        while(head){
            head=head->next;
            if(a->next&&a->next->next)
                a=a->next->next;
            else
                return false;
            if(head==a)
                return true;
        }
        return false;
    }
};