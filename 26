//26.cpp
class Solution {
public:
	int removeDuplicates(vector<int>& nums) {
		auto iter = nums.begin();
		if (nums.size() == 0)
			return 0;
		while (iter !=nums.end() - 1){
			if (*iter == *(iter + 1)) {
				iter = nums.erase(iter);
			}
			else {
				++iter;
			}
		}
		return set<int>(nums.begin(), nums.end()).size();
	}
};